//std.zh metadata

const float STD_VERSION = 1.7100;
const int STD_DATE_YEAR = 2020;
const int STD_DATE_MONTH = 7;
const int STD_DATE_DAY = 10;
const int STD_DATE_HOUR = 15;
const int STD_DATE_MINUTE = 26;

namespace std
{
	namespace versioninfo
	{
		float Get() { return STD_VERSION; }
		
		float GetDatestamp()
		{
			float dt = STD_DATE_MONTH;
			dt += STD_DATE_DAY * 100;
			dt += STD_DATE_YEAR / 10000;
			return dt;
		}
		float GetTimestamp()
		{
			float tm = STD_DATE_HOUR;
			tm += STD_DATE_MINUTE / 10000;
			return tm;
		}
	} //std::version
}